year = {2012}
}
@article{Eastin2009,
abstract = {"In this Letter we present a more general proof based on the structure of the Lie group of transversal unitary operators. Our result applies to all local-error-detecting quantum codes, that is, all quantum codes capable of detecting an arbitrary error on any single subsystem. An outline of the argument is as follows: The set of logical unitary product operators, G, is a Lie subgroup of the Lie group of unitary product operators, T. As a Lie group, G can be partitioned into cosets of the connected component of the identity, C; these cosets form a discrete set, Q. Using the fact that the Lie algebra of C is a subalgebra of T, it can be shown that the connected component of the identity acts trivially for any local- error-detecting code. This implies that the number of logi- cally distinct operators implemented by elements of G is limited to the cardinality of Q. Because of the compact- ness of T, this number must be finite. A finite number of operators can approximate infinitely many only up to some fixed accuracy; thus, G, the set of logical unitary product operators, cannot be universal. Transversal operators may be viewed as product operators with respect to a transversal partitioning of the code, so the ability to detect an arbitrary error on a transversal part implies the nonexistence of a universal, transversal encoded gate set."},
archivePrefix = {arXiv},
arxivId = {0811.4262},
author = {Eastin, Bryan and Knill, Emanuel},
doi = {10.1103/PhysRevLett.102.110502},
eprint = {0811.4262},
file = {::},
issn = {0031-9007},
journal = {Physical Review Letters},
month = {mar},
number = {11},
pages = {110502},
pmid = {19392181},
title = {{Restrictions on Transversal Encoded Quantum Gate Sets}},
url = {http://link.aps.org/doi/10.1103/PhysRevLett.102.110502},
volume = {102},
year = {2009}
}
@article{Kubica2015a,
abstract = {The topological color code and the toric code are two leading candidates for realizing$\backslash$r fault-tolerant quantum computation. Here we show that the color code on a d -dimensional closed$\backslash$r manifold is equivalent to multiple decoupled copies of the d -dimensional toric code up to local$\backslash$r unitary transformations and adding or removing ancilla qubits. Our result not only generalizes the$\backslash$r proven equivalence for d = 2, but also provides an explicit recipe of how to decouple independent$\backslash$r components of the color code, highlighting the importance of colorability in the construction of the$\backslash$r code. Moreover, for the d -dimensional color code with {\#}{\#}IMG{\#}{\#}$\backslash$r [http://ej.iop.org/images/1367-2630/17/8/083026/njp516957ieqn1.gif] {\{}{\$}d+1{\$}{\}} boundaries of {\#}{\#}IMG{\#}{\#}$\backslash$r [http://ej.iop.org/images/1367-2630/17/8/083026/njp516957ieqn2.gif] {\{}{\$}d+1{\$}{\}} distinct colors, we find$\backslash$r that the code is equivalent to multiple copies of the d -dimensional toric code which are attached$\backslash$r along a {\#}{\#}IMG{\#}{\#} [http://ej.iop.org/images/1367-2630/17/8/083026/njp516957ieqn3.gif] {\{}{\$}(d-1){\$}{\}}$\backslash$r -dimensional boundary. In particular, for d = 2, we show that the (triangular) color code with$\backslash$r boundaries is equivalent to the (folded) toric code with boundaries. We also find that the d$\backslash$r -dimensional toric code admits logical non-Pauli gates from the d th level of the Clifford$\backslash$r hierarchy, and thus saturates the bound by Bravyi and K{\"{o}}nig. In particular, we show that the logical$\backslash$r d -qubit control- Z gate can be fault-tolerantly implemented on the stack of d copies of the toric$\backslash$r code by a local unitary transformation.},
archivePrefix = {arXiv},
arxivId = {1503.02065},
author = {Kubica, Aleksander and Yoshida, Beni and Pastawski, Fernando},
doi = {10.1088/1367-2630/17/8/083026},
eprint = {1503.02065},
file = {::},
issn = {13672630},
journal = {New Journal of Physics},
keywords = {boundaries and domain walls,classification of quantum phases,color code,fault-tolerant quantum computation,quantum codes,topological order,transversal gates},
number = {8},

year = {2016}
}
@article{Selinger2012,
abstract = {We give an efficient randomized algorithm for approximating an arbitrary element of {\$}SU(2){\$} by a product of Clifford+{\$}T{\$} operators, up to any given error threshold {\$}\backslashepsilon{\textgreater}0{\$}. Under a mild hypothesis on the distribution of primes, the algorithm's expected runtime is polynomial in {\$}\backslashlog(1/\backslashepsilon){\$}. If the operator to be approximated is a {\$}z{\$}-rotation, the resulting gate sequence has {\$}T{\$}-count {\$}K+4\backslashlog{\_}2(1/\backslashepsilon){\$}, where {\$}K{\$} is approximately equal to {\$}10{\$}. We also prove a worst-case lower bound of {\$}K+4\backslashlog{\_}2(1/\backslashepsilon){\$}, where {\$}K=-9{\$}, so that our algorithm is within an additive constant of optimal for certain {\$}z{\$}-rotations. For an arbitrary member of {\$}SU(2){\$}, we achieve approximations with {\$}T{\$}-count {\$}K+12\backslashlog{\_}2(1/\backslashepsilon){\$}. By contrast, the Solovay-Kitaev algorithm achieves {\$}T{\$}-count {\$}O(\backslashlog{\^{}}c(1/\backslashepsilon)){\$}, where {\$}c{\$} is approximately {\$}3.97{\$}.},

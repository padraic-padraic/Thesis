journal = {Physical Review A - Atomic, Molecular, and Optical Physics},
number = {5},
pages = {1--11},
primaryClass = {quant-ph},
title = {{Magic-state distillation with low overhead}},
volume = {86},
year = {2012}
}
@article{Veitch2012,
abstract = {A central problem in quantum information is to determine the minimal physical resources that are$\backslash$n required for quantum computational speed-up and, in particular, for fault-tolerant quantum$\backslash$n computation. We establish a remarkable connection between the potential for quantum speed-up and the$\backslash$n onset of negative values in a distinguished quasi-probability representation, a discrete analogue of$\backslash$n the Wigner function for quantum systems of odd dimension. This connection allows us to resolve an$\backslash$n open question on the existence of bound states for magic state distillation: we prove that there$\backslash$n exist mixed states outside the convex hull of stabilizer states that cannot be distilled to$\backslash$n non-stabilizer target states using stabilizer operations. We also provide an efficient simulation$\backslash$n protocol for Clifford circuits that extends to a large class of mixed states, including bound$\backslash$n universal states.},
archivePrefix = {arXiv},
arxivId = {1201.1256},
author = {Veitch, Victor and Ferrie, Christopher and Gross, David and Emerson, Joseph},
doi = {10.1088/1367-2630/14/11/113011},
eprint = {1201.1256},
file = {::},
isbn = {1367-2630},
issn = {13672630},
journal = {New Journal of Physics},
title = {{Negative quasi-probability as a resource for quantum computation}},
volume = {14},
year = {2012}
}
@article{Watson2015,
abstract = {Two-level quantum systems, qubits, are not the only basis for quantum computation. Advantages exist in using qudits, d-level quantum systems, as the basic carrier of quantum information. We show that color codes, a class of topological quantum codes with remarkable transversality properties, can be generalized to the qudit paradigm. In recent developments it was found that in three spatial dimensions a qubit color code can support a transversal non-Clifford gate, and that in higher spatial dimensions additional non-Clifford gates can be found, saturating Bravyi and K$\backslash$"onig's bound [Phys. Rev. Lett. 110, 170503 (2013)]. Furthermore, by using gauge fixing techniques, an effective set of Clifford gates can be achieved, removing the need for state distillation. We show that the qudit color code can support the qudit analogues of these gates, and show that in higher spatial dimensions a color code can support a phase gate from higher levels of the Clifford hierarchy which can be proven to saturate Bravyi and K$\backslash$"onig's bound in all but a finite number of special cases. The methodology used is a generalisation of Bravyi and Haah's method of triorthogonal matrices [Phys. Rev. A 86 052329 (2012)], which may be of independent interest. For completeness, we show explicitly that the qudit color codes generalize to gauge color codes, and share the many of the favorable properties of their qubit counterparts.},
archivePrefix = {arXiv},
arxivId = {1503.08800},
author = {Watson, Fern H. E. and Campbell, Earl T. and Anwar, Hussain and Browne, Dan E.},
doi = {10.1103/PhysRevA.92.022312},
eprint = {1503.08800},
month = {mar},
title = {{Qudit Colour Codes and Gauge Colour Codes in All Spatial Dimensions}},
url = {http://arxiv.org/pdf/1503.08800.pdf http://arxiv.org/abs/1503.08800 http://dx.doi.org/10.1103/PhysRevA.92.022312},
year = {2015}
}

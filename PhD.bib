year = {2015}
}
@article{Raussendorf2007,
abstract = {We describe a fault-tolerant version of the one-way quantum computer using a cluster state in three$\backslash$r spatial dimensions. Topologically protected quantum gates are realized by choosing appropriate$\backslash$r boundary conditions on the cluster. We provide equivalence transformations for these boundary$\backslash$r conditions that can be used to simplify fault-tolerant circuits and to derive circuit identities in$\backslash$r a topological manner. The spatial dimensionality of the scheme can be reduced to two by converting$\backslash$r one spatial axis of the cluster into time. The error threshold is 0.75{\%} for each source in an error$\backslash$r model with preparation, gate, storage and measurement errors. The operational overhead is$\backslash$r poly-logarithmic in the circuit size.},
archivePrefix = {arXiv},
arxivId = {quant-ph/0703143},

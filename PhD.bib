url = {http://arxiv.org/abs/quant-ph/0505036 http://dx.doi.org/10.1088/1367-2630/7/1/170 http://stacks.iop.org/1367-2630/7/i=1/a=170?key=crossref.cbc2ae8988c042198e839c1b114df924},
volume = {7},
year = {2005}
}

pages = {289--301},
title = {{A Fast Approach for Overcomplete Sparse Decomposition Based on Smoothed L0 Norm}},
url = {http://arxiv.org/abs/0809.2508 http://dx.doi.org/10.1109/TSP.2008.2007606 http://ieeexplore.ieee.org/lpdocs/epic03/wrapper.htm?arnumber=4663911},
